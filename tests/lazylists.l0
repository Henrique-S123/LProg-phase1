let fibo = fn a,b => {a:?(fibo (b) (a+b))};
let fibogen = fibo (0) (1);
let count = box 20;
let lv = box(fibogen);
while (*count ~= 0) {
    match *lv {
    | nil -> print("never happens")
    | v::tail -> println(v); lv := tail};
count := *count - 1};;
